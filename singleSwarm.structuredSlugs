[INPUT]
active:1...6

[OUTPUT]
known_room:0...6
known:0...1
pos:0...6

[ENV_INIT]
active = 1

[SYS_INIT]
pos = 1
known_room = 0
known = 0

[SYS_TRANS]
(((pos = 0) & (pos' >0)) | ((pos >0) & (pos' = 0)))
(((pos = active) & (known'=1 & known_room'=pos)) | ((pos != active) & (known'=0 & known_room'=0)))
((known = 1) -> (pos' = known_room))

[ENV_TRANS]
active'>0

[ENV_LIVENESS]


[SYS_LIVENESS]
